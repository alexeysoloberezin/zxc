@charset "UTF-8";

.modal-buy {
  display: none;
}

.est {
  display: none;
}

@media (min-width: 460px) {
  div .calculator__sel-list {
    display: none;
  }
}

a,
button {
  border: none;
  outline: none;
  text-decoration: none;
}

a {
  display: block;
}

a:hover,
a:focus,
a:active,
a:visited {
  text-decoration: none;
}

body {
  font-family: "Open Sans", sans-serif;
}

.wrapper {
  width: 100%;
  min-height: 100vh;
  overflow: hidden;
}

body {
  color: #1A1919;
}

.w100 {
  width: 100%;
}

.main-title {
  color: #1A1919;
  font-family: Russo One;
  font-size: 36px;
  line-height: 43px;
  margin: 0 auto;
  max-width: 630px;
  text-align: center;
}

.burger {
  width: 50px;
  height: 50px;
  z-index: 30;
  position: absolute;
  top: 30px;
  right: 20px;
  flex-shrink: 0;
  display: none;
  cursor: pointer;
}

.burger-line__top,
.burger-line__center,
.burger-line__bottom {
  width: 47px;
  height: 5px;
  background: #59B130;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.burger-line__top {
  margin-top: -12px;
}

.burger-line__bottom {
  margin-top: 12px;
}

.header {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}

.header__row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 105px;
  z-index: 20;
  background: #fff;
}

.header__ph {
  display: none;
}

.header__logo {
  width: 150px;
  height: 87px;
  flex: 0 0 150px;
  position: relative;
  z-index: 4;
}

.header__logo-img {
  width: 100%;
}

.header__logo-title {
  font-weight: 600;
  color: #155425;
  line-height: 19px;
  font-size: 14px;
  white-space: nowrap;
}

.header__nav {
  margin-right: -40px;
}

.header__list {
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.header__link {
  position: relative;
  font-size: 16px;
  line-height: 22px;
  color: #1A1919;
  height: 105px;
  display: flex;
  align-items: center;
}

.header__link:before {
  content: "";
  position: absolute;
  bottom: 0;
  display: block;
  width: 100%;
  height: 2px;
  background: #59B130;
  transform: scale(0);
  transition: 0.3s ease-in-out;
}

.header__link:hover:before {
  transform: scale(1);
}

.header__link:hover {
  color: #1A1919;
}

.header__contacts {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  margin-top: 3px;
}

.header__inner {
  display: flex;
  align-items: center;
}

.header__nomber {
  font-weight: 600;
  font-size: 18px;
  line-height: 25px;
  color: #1A1919;
}

.header__mail {
  color: #ACACAC;
  font-size: 18px;
  line-height: 25px;
  margin-top: 4px;
  transition: 0.25s;
}

.header__mail:hover {
  color: #5A5959;
}

.svg-tel {
  width: 20px;
  height: 20px;
  margin-right: 13px;
  margin-top: 4px;
}

.svg-tel path {
  fill: #59B130;
}

.header-body {
  background-image: url(../image/header-bg.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  min-height: calc(100vh - 105px);
  padding-bottom: 50px;
}

.header-body__title {
  font-family: Russo One;
  font-size: 38px;
  line-height: 46px;
  color: #1A1919;
  max-width: 585px;
  margin-top: 230px;
}

.header-body__wrapper {
  margin-top: 20px;
  display: flex;
  flex-wrap: wrap;
}

.form-header {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.form-header__type {
  width: 42%;
  margin-right: 25px;
  position: relative;
}

.form-header__type:after {
  content: "";
  position: absolute;
  bottom: 18px;
  right: 27px;
  width: 8px;
  height: 8px;
  background: url(../image/svg/treu.svg);
  pointer-events: none;
}

.form-header__block {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 15px;
}

.form-header__select {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  cursor: pointer;
}

.form-header__input {
  width: 100%;
  height: 45px;
  background: #fff;
  border-radius: 30px;
  border: 1px solid #B4B4B4;
  padding: 0 20px;
  font-style: 16px;
  line-height: 19px;
  color: #1A1919;
  margin-top: 15px;
}

.form-header__text {
  font-weight: 700;
  font-size: 16px;
  line-height: 22px;
  color: #1A1919;
}

.form-header__input-area {
  padding-right: 45px;
}

.form-header__area {
  width: 23%;
  margin-right: 25px;
  position: relative;
}

.form-header__area .form-header__text {
  margin-left: -2px;
}

.form-header__area:after {
  content: "М²";
  position: absolute;
  bottom: 11px;
  right: 15px;
  font-size: 17px;
  font-weight: 400;
}

.form-header__quantity {
  width: 23%;
  position: relative;
}

.form-header__quantity .form-header__text {
  margin-left: -2px;
}

.form-header__phone {
  width: 42%;
}

.form-header__btn {
  height: 60px;
  background: #59B130;
  width: 42%;
  border-radius: 30px;
  font-family: Russo One;
  font-size: 17px;
  line-height: 19px;
  color: #fff;
  transition: 0.25s;
  margin-top: 25px;
}

.form-header__btn:hover {
  background: #5ca33b;
}

#form-header__quantity {
  padding: 0 0 0 45%;
  font-size: 16px;
  font-weight: 600;
}

.rem {
  width: 30px;
  height: 30px;
  position: absolute;
  bottom: 7px;
  left: 15px;
  cursor: pointer;
}

.rem:Before {
  content: "";
  position: absolute;
  bottom: 50%;
  left: 50%;
  width: 15px;
  height: 2px;
  background: #59B130;
  transform: translate(-50%, 50%);
}

.add {
  width: 30px;
  height: 30px;
  position: absolute;
  bottom: 7px;
  right: 15px;
  cursor: pointer;
}

.add:Before {
  content: "";
  position: absolute;
  bottom: 50%;
  left: 50%;
  width: 15px;
  height: 2px;
  background: #59B130;
  transform: translate(-50%, 50%);
}

.add:after {
  content: "";
  position: absolute;
  bottom: 50%;
  left: 50%;
  width: 15px;
  height: 2px;
  background: #59B130;
  transform: translate(-50%, 50%) rotate(90deg);
}

.header__inner-two {
  display: none;
}

.header__mail-two {
  display: none;
  color: #ACACAC;
  font-size: 18px;
  line-height: 25px;
  margin-top: 4px;
}

.main {
  display: flex;
  flex-direction: column;
}

.cleaning {
  padding-top: 100px;
  padding-bottom: 70px;
  text-align: center;
}

.cleaning__subtitle {
  font-size: 16px;
  line-height: 22px;
  color: #1A1919;
  margin-top: 10px;
  max-width: 610px;
  margin: 0 auto;
  margin-top: 20px;
}

.cleaning__wrapper {
  margin-top: 75px;
}

.cleaning__inner {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  min-height: 368px;
}

.cleaning__image {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
}

.cleaning__image img,
.cleaning__image svg {
  width: 70px;
  height: 70px;
}

.cleaning__title {
  color: #59B130;
  font-weight: 700;
  font-size: 20px;
  line-height: 27px;
  margin-top: 20px;
}

.cleaning__text {
  font-size: 14px;
  color: #1A1919;
  line-height: 19px;
  margin-top: 20px;
}

.cleaning__price {
  margin-top: 21px;
  flex-grow: 1;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  color: #1A1919;
  font-size: 22px;
  line-height: 30px;
  font-weight: 700;
}

.cleaning__order,
.cleaning__details {
  display: flex;
  align-items: center;
  justify-content: center;
  transition: 0.25s;
  width: 230px;
  padding: 10px;
  min-height: 40px;
  border-radius: 50px;
  font-family: Russo One;
  margin: 0 auto;
}

.cleaning__order {
  color: #fff;
  background: #59B130;
  margin: 22px auto 0;
}

.cleaning__order:hover,
.cleaning__order:focus {
  color: #fff;
  text-decoration: none;
  background: #488f27;
  outline: none;
}

.cleaning__details {
  color: #59B130;
  border: 1px solid #59B130;
  margin: 10px auto 0;
}

.cleaning__details:hover {
  color: #59B130;
  text-decoration: none;
  box-shadow: 0 0 2px 2px #bddbaf;
  outline: none;
}

.cleaning__details:active {
  color: #59B130;
}

.cleaning__details:focus {
  color: #59B130;
  text-decoration: none;
  box-shadow: 0 0 3px 3px #bddbaf;
  outline: none;
}

.includes {
  padding-top: 50px;
}

.includes__wrapper {
  background-image: url(../image/bg3.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  margin-top: 30px;
}

.includes__mob-btn {
  display: none;
}

.includes__inner {
  background: #fff;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.25);
  border-radius: 30px;
  width: 90%;
  margin: 45px auto;
  padding: 50px 100px;
}

.includes__block {
  display: flex;
}

.includes__label {
  margin: 0 10px;
  color: #1A1919;
  font-weight: 700;
  font-size: 18px;
  line-height: 25px;
  transition: 0.25s ease-in-out;
  border-bottom: 2px solid #fff;
  cursor: pointer;
  padding-bottom: 8px;
}

.includes__place-inp {
  width: 1px;
  height: 1px;
  visibility: hidden;
  position: absolute;
}

.includes__place-label {
  padding: 3px 20px;
  border-radius: 50px;
  margin-left: 15px;
  border: 1px solid #ccc;
  transition: 0.25s ease-in-out;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  font-size: 16px;
  min-height: 40px;
  color: #1A1919;
}

.includes__place-label:nth-child(2) {
  margin-left: 0;
}

.includes__inp {
  width: 1px;
  height: 1px;
  visibility: hidden;
  position: absolute;
}

.includes__mob-select {
  position: absolute;
  visibility: hidden;
  z-index: -3;
}

.includes__place {
  margin-top: 25px;
  display: flex;
}

.includes__title {
  font-weight: 700;
  color: #1A1919;
  font-size: 25px;
  line-height: 34px;
  margin-top: 25px;
  margin-bottom: 20px;
}

.includes__list {
  display: none;
  flex-wrap: wrap;
}

.includes__item {
  position: relative;
  width: 50%;
  font-size: 14px;
  line-height: 18px;
  color: #1A1919;
  padding: 6px 6px 6px 25px;
  display: flex;
}

.includes__item:before {
  content: "";
  position: absolute;
  width: 16px;
  height: 16px;
  background-image: url(../image/svg/ok3.svg);
  background-repeat: no-repeat;
  background-position: 50% 50%;
  left: 0;
  top: 0;
  margin-top: 7px;
}

.apm-sel {
  display: none;
}

.includes__inp:checked+.includes__label {
  color: #59B130;
  border-bottom: 2px solid #59B130;
}

.includes__place-inp:checked+.includes__place-label {
  background: #59B130;
  color: #fff;
}

.calculator {
  padding-top: 95px;
  padding-bottom: 80px;
}

.calculator__inr {
  margin-top: 35px;
}

.calculator__subtitle {
  font-size: 16px;
  line-height: 22px;
  color: #1A1919;
  max-width: 500px;
  margin: 0 auto;
  text-align: center;
}

.calculator__wrapper {
  display: flex;
  margin-top: 25px;
}

.calculator .calcl-title {
  padding-bottom: 10px;
}

.calculator__inner {
  width: 50%;
  padding-top: 30px;
}

.calculator__inner:first-child {
  border-right: 1px solid #989898;
}

.calculator__title {
  font-weight: 700;
  color: #1A1919;
  font-size: 18px;
  width: 100%;
  flex-grow: 1;
  padding-bottom: 25px;
}

.apartment {
  display: flex;
  flex-wrap: wrap;
}

.apartment__wrapper {
  display: flex;
  width: 50%;
  padding-right: 15px;
}

.apartment__quantity-inp {
  border-bottom: 1px solid #000;
  text-align: center;
  width: 60px;
  margin-left: 10px;
  height: 25px;
}

.apartment__area-inp {
  border-bottom: 1px solid #000;
  text-align: center;
  width: 60px;
  margin-left: 10px;
  height: 25px;
}

.cal-type {
  padding: 0px 0 40px 0;
  display: flex;
  justify-content: space-between;
  width: 85%;
}

.cal-type__wrapper {
  padding-right: 5px;
}

.cal-type__radio {
  display: none;
}

.cal-type__label {
  position: relative;
  padding-left: 30px;
  max-width: 100%;
  display: flex;
  align-items: flex-start;
}

.cal-type__label:before {
  position: absolute;
  content: "";
  width: 22px;
  left: 0;
  top: 0;
  height: 22px;
  border: 1px solid #9F9F9F;
  border-radius: 50%;
}

.cal-type__label:after {
  position: absolute;
  content: "";
  width: 14px;
  left: 4px;
  top: 0;
  margin-top: 4px;
  height: 14px;
  background: #59B130;
  border-radius: 50%;
  transform: scale(0);
  transition: 0.4s ease-in-out;
}

.cal-type__radio:checked+.cal-type__label:after {
  transform: scale(1);
}

.cal-extra__main {
  width: 390px;
  border: 1px solid #C6C6C6;
  border-radius: 30px;
  height: 45px;
  display: flex;
  align-items: center;
  padding: 5px 25px;
  cursor: pointer;
  font-size: 16px;
  position: relative;
}

.cal-extra__main:before {
  content: "";
  position: absolute;
  background: url(../image/svg/treu.svg);
  right: 20px;
  top: 50%;
  margin-top: -4px;
  width: 8px;
  height: 8px;
}

.cal-extra__list {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  width: 390px;
  height: 265px;
  border: 1px solid #C6C6C6;
  border-radius: 0 0 5px 25px;
  overflow-x: hidden;
  display: none;
  border-top: none;
}

.cal-extra__list li:first-child {
  margin-top: 15px;
}

.cal-extra__list li:last-child {
  margin-bottom: 20px;
}

.cal-extra__item {
  padding: 7px 23px;
  display: flex;
  justify-content: space-between;
}

.cal-extra__line {
  flex-grow: 1;
  border-bottom: 0.5px solid #c2c2c2;
}

.cal-extra .cl {
  display: none;
  cursor: pointer;
}

.cal-extra .cl-box {
  padding-left: 25px;
  position: relative;
  flex-shrink: 1;
}

.cal-extra .cl-box:before {
  content: "";
  position: absolute;
  width: 18px;
  height: 18px;
  left: 0;
  border-radius: 5px;
  border: 1px solid #B8B8B8;
  margin-top: 2px;
  background-repeat: no-repeat;
  background-position: 70% 50%;
}

.cal-extra__text {
  transition: 0.2s;
  cursor: pointer;
  flex-shrink: 0;
}

.cl:checked+.cl-box:before {
  background-image: url(../image/svg/ok.svg);
}

.cl:checked~.cal-extra__text {
  color: #59B130;
}

.cal-extra__list::-webkit-scrollbar {
  width: 3px;
  margin-right: 30px;
}

.cal-extra__list::-webkit-scrollbar-thumb {
  background: #adadad;
}

.cal-extra__list::-webkit-scrollbar-button {
  padding: 320px;
}

