.aplication {
  background-image: url(../image/bgajax.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}

.aplication__wrapper {
  width: 80%;
  background: #fff;
  border-radius: 30px;
  margin: 55px auto;
}

.aplication__title {
  padding-top: 27px;
}

.aplication__subtitle {
  color: #1A1919;
  font-size: 20px;
  line-height: 27px;
  text-align: center;
  max-width: 450px;
  margin: 0 auto;
}

.aplication__form {
  display: flex;
  justify-content: space-between;
  padding: 30px 90px;
}

.aplication__block {
  margin: 0 10px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.aplication__inner {
  display: flex;
  flex-direction: column;
  padding-bottom: 27px;
}

.aplication__label {
  font-weight: 600;
  line-height: 19px;
  color: #1A1919;
}

.aplication__inp {
  border-bottom: 1px solid #989898;
  font-size: 14px;
  margin-top: 3px;
}

.aplication__room {
  position: relative;
}

.aplication__remove {
  position: absolute;
  bottom: 4px;
  left: 4px;
  width: 15px;
  height: 15px;
  z-index: 2;
  cursor: pointer;
}

.aplication__remove:before {
  content: "";
  position: absolute;
  width: 14px;
  height: 2px;
  background: #59B130;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.aplication__add {
  position: absolute;
  bottom: 4px;
  right: 4px;
  width: 15px;
  height: 15px;
  cursor: pointer;
  z-index: 2;
}

.aplication__add:before,
.aplication__add:after {
  content: "";
  position: absolute;
  width: 14px;
  height: 2px;
  background: #59B130;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.aplication__add:After {
  transform: translate(-50%, -50%) rotate(90deg);
}

.aplication__room-inp {
  width: 90px;
  margin: 0 auto;
  text-align: center;
}

.aplication__type {
  position: relative;
}

.aplication__type:before {
  content: "";
  position: absolute;
  bottom: 8px;
  right: 5px;
  width: 8px;
  height: 8px;
  background-image: url(../image/svg/treu.svg);
  background-repeat: no-repeat;
